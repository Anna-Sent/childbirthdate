<?xml version="1.0" encoding="utf-8"?><!--suppress CheckTagEmptyBody, CheckTagEmptyBody -->
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <declare-styleable name="AppTheme">
        <attr name="menuIconHelp" format="reference" />
        <attr name="menuIconSettings" format="reference" />
        <attr name="menuIconEmail" format="reference" />
        <attr name="iconGoToToday" format="reference" />
        <attr name="iconNextItem" format="reference" />
        <attr name="iconPrevItem" format="reference" />
        <attr name="iconExpand" format="reference" />
        <attr name="iconCollapse" format="reference" />
        <attr name="iconAdd" format="reference" />
        <attr name="iconDelete" format="reference" />
        <attr name="iconEdit" format="reference" />
    </declare-styleable>

    <style name="AppBaseTheme" parent="android:Theme">
        <item name="numberPickerStyle">@style/Widget.NumberPicker</item>
        <item name="menuIconHelp">@drawable/ic_menu_help_light</item>
        <item name="menuIconSettings">@drawable/ic_menu_settings_light</item>
        <item name="menuIconEmail">@drawable/ic_menu_email_light</item>
        <item name="iconGoToToday">@drawable/ic_go_to_today_light</item>
        <item name="iconNextItem">@drawable/ic_next_item_light</item>
        <item name="iconPrevItem">@drawable/ic_previous_item_light</item>
        <item name="iconExpand">@drawable/ic_expand_light</item>
        <item name="iconCollapse">@drawable/ic_collapse_light</item>
        <item name="iconAdd">@drawable/ic_new_light</item>
        <item name="iconDelete">@drawable/ic_remove_content_light</item>
        <item name="iconEdit">@drawable/ic_edit_light</item>
    </style>

    <style name="AppBaseThemeLight" parent="android:Theme.Light">
        <item name="numberPickerStyle">@style/Widget.NumberPicker</item>
        <item name="menuIconHelp">@drawable/ic_menu_help_light</item>
        <item name="menuIconSettings">@drawable/ic_menu_settings_light</item>
        <item name="menuIconEmail">@drawable/ic_menu_email_light</item>
        <item name="iconGoToToday">@drawable/ic_go_to_today_light</item>
        <item name="iconNextItem">@drawable/ic_next_item_light</item>
        <item name="iconPrevItem">@drawable/ic_previous_item_light</item>
        <item name="iconExpand">@drawable/ic_expand_light</item>
        <item name="iconCollapse">@drawable/ic_collapse_light</item>
        <item name="iconAdd">@drawable/ic_new_light</item>
        <item name="iconDelete">@drawable/ic_remove_content_light</item>
        <item name="iconEdit">@drawable/ic_edit_light</item>
    </style>

    <style name="AppTheme" parent="AppBaseTheme"></style>

    <style name="AppThemeLight" parent="AppBaseThemeLight"></style>

    <style name="DialogBaseTheme" parent="android:Theme.Dialog">
        <item name="numberPickerStyle">@style/Widget.NumberPicker</item>
    </style>

    <style name="DialogBaseThemeLight" parent="android:Theme.Dialog">
        <item name="numberPickerStyle">@style/Widget.NumberPicker</item>
    </style>

    <color name="opaque_50">#80000000</color>

    <style name="DialogTheme" parent="DialogBaseTheme">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowBackground">@color/opaque_50</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <style name="DialogThemeLight" parent="DialogBaseThemeLight">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowBackground">@color/opaque_50</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

</resources>